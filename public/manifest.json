{
   "manifest_version": 3,
   "name": "My Extension",
   "version": "1.0",
   "description": "A description of your extension",
   "permissions": ["windows", "tabs", "activeTab", "webNavigation", "storage"],
   "background": {
      "service_worker": "background.js"
   },
   "action": {
      "default_popup": "index.html"
   },
   "web_accessible_resources": [
      {
         "resources": ["main-page.html", "main-page.bundle.js"],
         "matches": ["<all_urls>"]
      }
   ]
}
